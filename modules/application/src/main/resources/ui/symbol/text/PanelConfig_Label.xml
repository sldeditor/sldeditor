            label="geoserver.label.goodnessOfFit" valueOnly="true"
            default="org.geotools.styling.TextSymbolizer2.DEFAULT_GOODNESS_OF_FIT" />
        <FieldConfigInteger id="LABEL_GRAPHIC_MARGIN"
            label="geoserver.label.graphicMargin" valueOnly="true"
            default="0" minValue="0" />
        <FieldConfigEnum id="LABEL_GRAPHIC_RESIZE"
            label="geoserver.label.graphicResize" valueOnly="true"
            default="none">
            <ValueList>
                <Value>
                    <Item id="none" label="geoserver.label.graphicResize.none" />
                    <Item id="proportional"
                        label="geoserver.label.graphicResize.proportional" />
                    <Item id="stretch"
                        label="geoserver.label.graphicResize.stretch" />
                </Value>
            </ValueList>
        </FieldConfigEnum>
        <FieldConfigBoolean id="LABEL_GROUP"
            label="geoserver.label.group" valueOnly="true"
            default="org.geotools.styling.TextSymbolizer2.DEFAULT_GROUP" />
