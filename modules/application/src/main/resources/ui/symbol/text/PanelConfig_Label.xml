            label="geoserver.label.polygonAlign" valueOnly="true"
            default="manual">
            <ValueList>
                <Value>
                    <Item id="manual" label="geoserver.label.polygonAlign.manual" />
                    <Item id="ortho" label="geoserver.label.polygonAlign.ortho" />
                    <Item id="mbr" label="geoserver.label.polygonAlign.mbr" />
