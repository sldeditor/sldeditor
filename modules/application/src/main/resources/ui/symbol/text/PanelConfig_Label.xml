<?xml version="1.0" encoding="UTF-8"?>
<PanelConfig vendorOption="com.sldeditor.common.vendoroption.GeoServerVendorOption"
    start="2.4.1"
    localisation="com.sldeditor.ui.detail.vendor.geoserver.text.VOGeoServerLabelling">
    <Group id="VO_LABELLING" label="geoserver.label" showLabel="true">
        <FieldConfigBoolean id="LABEL_ALLOW_OVERRUNS"
            label="geoserver.label.allowOverruns" valueOnly="true"
            default="org.geotools.styling.TextSymbolizer2.DEFAULT_ALLOW_OVERRUNS" />
        <FieldConfigInteger id="LABEL_AUTO_WRAP"
            label="geoserver.label.autowrap" valueOnly="true"
            default="org.geotools.styling.TextSymbolizer2.DEFAULT_AUTO_WRAP" />
        <FieldConfigBoolean id="LABEL_CONFLICT_RESOLUTION"
            label="geoserver.label.conflictResolution" valueOnly="true"
            default="org.geotools.styling.TextSymbolizer2.DEFAULT_CONFLICT_RESOLUTION" />
        <FieldConfigBoolean id="LABEL_FOLLOW_LINE"
            label="geoserver.label.followLine" valueOnly="true"
            default="org.geotools.styling.TextSymbolizer2.DEFAULT_FOLLOW_LINE" />
        <FieldConfigBoolean id="LABEL_FORCE_LEFT_TO_RIGHT"
            label="geoserver.label.forceLeftToRight" valueOnly="true"
            default="org.geotools.styling.TextSymbolizer2.DEFAULT_FORCE_LEFT_TO_RIGHT" />
